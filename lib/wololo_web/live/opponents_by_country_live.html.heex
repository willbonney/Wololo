<div id="opponents-by-country">
  <h1 class="font-time-traveler-pal font-black text-6xl mb-4">Opponents By Country</h1>

  <%= if @loading do %>
    <.spinner />
  <% else %>
    <!-- render data here -->
    <canvas id="countries" phx-hook="OpponentsByCountry" data-points={Jason.encode!(@countries)}>
    </canvas>
  <% end %>
</div>
